@model MVC.CMN.Models.MessageBoard.MessageBoardViewModel
@{
    ViewBag.Title = "Index";
}

<div>
    <div class="panel rounded border border-info col-4 mx-auto panel-transparent p-2 m-2">
        <h2 class="text-center">REAL MessageBoard</h2>
    </div>
</div>

<table class="mx-auto col-10">

    @{
        foreach (var board in Model.Boarditems)
        {
            @Html.Partial("_BoardCell", board)

        }
    }
</table>

<div>
    <ul class="breadcrumb">
        <li><a href="#">Main</a></li>
    </ul>
</div>
